			function placeOrder() {
				pageLoadingFrame("show");
				setTimeout(function() {
					pageLoadingFrame("hide");
				}, 1500);

				showOrderDetailsBlock();
				$("#alertblock").show();

				setTimeout(function() {
					$("#alertblock").fadeOut();
				}, 5000);
				//reset place order button to disabled

			}

			function showSearchDistributor() {
				$("#activeDistributor").hide();
				$("#searchDistributor").show();
			}

			function showDistributorActivity(orderStatus) {

				/*pageLoadingFrame("show");
				 setTimeout(function() {
				 pageLoadingFrame("hide");

				 }, 1000);
				 */
				if (orderStatus == 'open') {
					reset();
					$("#ordersOpen").addClass("widget-dark-gray");
					$("#orderlistlabel").html("Open Orders");
					$("#order_value").html("₦4,981,423.00");
					$("#order_total_1").html("₦5,381,423.00");

				} else if (orderStatus == 'paid') {
					reset();
					$("#ordersPaid").addClass("widget-dark-gray");
					$("#orderlistlabel").html("Approved Orders");
					$("#order_value").html("₦1,181,323.00");
					$("#order_total_1").html("₦1,581,323.00");
				} else if (orderStatus == 'collected') {
					reset();
					$("#ordersCollected").addClass("widget-dark-gray");
					$("#orderlistlabel").html("Paid Orders");
					$("#order_value").html("₦2,181,323.00");
					$("#order_total_1").html("₦2,581,323.00");
				} else if (orderStatus == 'completed') {
					reset();
					$("#ordersCollected").addClass("widget-dark-gray");
					$("#orderlistlabel").html("Completed Orders");
					$("#order_value").html("₦3,144,320.00");
					$("#order_total_1").html("₦3,544,320.00");
				} else if (orderStatus == 'drafts') {
					reset();
					$("#ordersDrafts").addClass("widget-dark-gray");
					$("#orderlistlabel").html("Drafts");
					$("#order_value").html("₦10,500.00");
					$("#order_total_1").html("₦410,500.00");
				}

				$("#distributorSummary").hide();
				$("#distributorActivity").fadeOut();
				$("#distributorActivity").fadeIn();
			}

			function showDistributorSummary() {

				$("#distributorActivity").hide();
				$("#distributorSummary").fadeIn();
			}

			function reset() {
				$("#ordersOpen").removeClass("widget-dark-gray");
				$("#ordersPaid").removeClass("widget-dark-gray");
				$("#ordersCollected").removeClass("widget-dark-gray");
				$("#ordersDrafts").removeClass("widget-dark-gray");

				$("#ordersOpen").addClass("widget-gray");
				$("#ordersPaid").addClass("widget-gray");
				$("#ordersCollected").addClass("widget-gray");
				$("#ordersDrafts").addClass("widget-gray");
			}

		</script>

